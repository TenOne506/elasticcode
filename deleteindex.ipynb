    "from elasticsearch_dsl import connections, Index\n",
    "\n",
    "# 1. 创建连接（修正图片中的拼写错误）\n",
    "connections.create_connection(hosts=\"http://localhost:9200\")\n",
    "\n",
    "# 2. 删除books索引\n",
    "index = Index('books')\n",
    "if index.exists():\n",
    "    index.delete()  # 执行删除\n",
    "    print(\"✅ 索引删除成功\")\n",
    "else:\n",
    "    print(\"⚠️ 索引不存在\")"
   ]
